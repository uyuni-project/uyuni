<?xml version="1.0"?>

<schema targetNamespace="http://rhn.redhat.com"
	xmlns="http://www.w3.org/1999/XMLSchema" xmlns:rhn="http://rhn.redhat.com">
	<attribute name="traceback_mail">
		<simpleType baseType="string">
		<matchesExpression value="(.+)@(.+)(\..+)*" />
		<minLength value="1" />
		<maxLength value="100" />
		</simpleType>
	</attribute>
	<attribute name="java|hostname">
		<simpleType baseType="string">
		<minLength value="1" />
		<maxLength value="100" />
			<matchesExpression value="(.+)(\..+)*" />
		</simpleType>
	</attribute>
</schema>
