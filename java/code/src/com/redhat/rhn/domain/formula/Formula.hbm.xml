<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.redhat.rhn.domain.formula.Formula" table="suseFormula">
        <id name="id" type="long" column="id">
            <!--<generator class="sequence">
                <param name="sequence">suse_formula_id_seq</param>
            </generator>-->
			<generator class="increment"></generator>
        </id>
	    <many-to-one name="org" class="com.redhat.rhn.domain.org.Org" column="org_id"/>
	    <property name="formulaName" column="formula_name" type="string" not-null="true" length="256"/>
	    <property name="serverId" column="server_id" type="long" not-null="true"/>
	    <property name="content" column="formula_content" type="string" not-null="true"/>
    </class>
</hibernate-mapping>
