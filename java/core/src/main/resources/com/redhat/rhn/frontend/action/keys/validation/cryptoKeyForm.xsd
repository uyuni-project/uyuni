<?xml version="1.0"?>

<schema targetNamespace="http://rhn.redhat.com"
        xmlns="http://www.w3.org/1999/XMLSchema"
        xmlns:rhn="http://rhn.redhat.com">

  <attribute name="description">
    <simpleType baseType="string">
      <minLength value="1" />
      <maxLength value="40" />
    </simpleType>
  </attribute>

  <attribute name="type">
    <simpleType baseType="string">
        <minLength value="1"/>
        <maxLength value="16"/>
    </simpleType>
  </attribute>


</schema>
