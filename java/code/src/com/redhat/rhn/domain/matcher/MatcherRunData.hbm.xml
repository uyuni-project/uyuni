<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
        PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.redhat.rhn.domain.matcher.MatcherRunData"
           table="suseMatcherRunData">
        <id name="id" type="long" column="id">
            <generator class="sequence">
                <param name="sequence">suse_matcher_run_data_id_seq</param>
            </generator>
        </id>

        <property name="input" type="string"/>
        <property name="output" type="string"/>
        <property name="subscriptionReport" type="string"/>
        <property name="messageReport" type="string"/>
        <property name="unmatchedSystemReport" type="string"/>
    </class>

    <query name="MatcherRunData.deleteAll">
        <![CDATA[
            DELETE FROM com.redhat.rhn.domain.matcher.MatcherRunData
        ]]>
    </query>
</hibernate-mapping>

