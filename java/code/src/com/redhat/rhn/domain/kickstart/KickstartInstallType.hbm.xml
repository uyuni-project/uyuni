<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping
PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.redhat.rhn.domain.kickstart.KickstartInstallType"
        table="rhnKsInstallType" mutable="false">
        <cache usage="read-only"/>
        <id name="id" type="long" column="id">
            <meta attribute="scope-set">protected</meta>
            <generator class="assigned" />
        </id>
        <property name="name" type="string" column="name" />
        <property name="label" type="string" column="label" />
        <property name="created" type="date" column="created" />
        <property name="modified" type="date" column="modified" />
    </class>

    <query name="KickstartInstallType.loadAll">
        <![CDATA[from com.redhat.rhn.domain.kickstart.KickstartInstallType as t order by t.label desc]]>
    </query>
    <query name="KickstartInstallType.findById">
        <![CDATA[from com.redhat.rhn.domain.kickstart.KickstartInstallType as t where t.id = :id]]>
    </query>
    <query name="KickstartInstallType.findByLabel">
        <![CDATA[from com.redhat.rhn.domain.kickstart.KickstartInstallType as t where t.label = :label]]>
    </query>
</hibernate-mapping>
