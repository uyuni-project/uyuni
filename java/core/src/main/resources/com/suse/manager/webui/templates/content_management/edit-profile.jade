include ../common.jade

#image-profile-edit

script(type='text/javascript').
    window.csrfToken = "#{csrf_token}";
    window.profileId = "#{profileId}";
    window.activationKeys = !{activationKeys};
    window.customDataKeys = !{customDataKeys};
    window.imageTypesDataFromTheServer = !{imageTypesDataFromTheServer};

script(type='text/javascript').
    spaImportReactPage('images/image-profile-edit')
        .then(function(module) { module.renderer('image-profile-edit') });
