include ../common.jade
#packages-list

script(type='text/javascript').
    window.csrfToken = "#{csrf_token}";

script(type='text/javascript').
    spaImportReactPage('packages/list')
        .then(function(module) {
            module.renderer('packages-list', '#{docsLocale}', {
                selected: !{selected},
                selectedChannel: "#{selectedChannel}"
            })
        });