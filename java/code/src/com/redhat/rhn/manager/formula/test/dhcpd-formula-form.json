{
  "dhcpd": {
    "$type": "hidden-group",
    "domain_name": {
      "$placeholder": "example1.org",
      "$optional": false,
      "$default": "test-name",
      "$ifempty": null
    },
    "domain_name_servers": {
      "$placeholder": "example.org",
      "$ifempty": "Test",
      "$default": null

    },
    "listen_interfaces": {
      "$help": "Comma separated list of interfaces to listen on",
      "$default": "em2"
    },
    "max_lease_time": {
      "$default": 20001,
      "$type": "number",
      "$ifempty": 1
    },
    "default_lease_time": {
      "$default": 20000,
      "$type": "number",
      "$ifempty": 2
    },
    "subnets": {
      "$type": "group",
      "network1": {
        "$type": "group",
        "$name": "Network configuration",
        "network": {
          "$type": "text",
          "$default": "192.168.128.0"
        },
        "netmask": {
          "$default": "255.255.128.0",
          "$type": "text"
        },
        "range": {
          "$default": [
            "192.168.242.51",
            "192.168.243.151"
          ]
        },
        "broadcast_address": {
          "$default": "192.168.255.255"
        },
        "routers": {
          "$default": [
            "192.168.243.1"
          ]
        },
        "next_server": {
          "$default": "192.168.255.255"
        },
        "filename": {
          "$default": "/boot/pxelinux.0",
          "$type": "text"
        }
      }
    }
  }
}