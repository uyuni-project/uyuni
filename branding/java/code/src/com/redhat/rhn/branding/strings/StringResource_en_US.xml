<?xml version="1.0" encoding="utf-8"?>
<xliff xmlns="urn:oasis:names:tc:xliff:document:1.1" xmlns:xyz="urn:appInfo:Items" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="urn:oasis:names:tc:xliff:document:1.1 http://www.oasis-open.org/committees/xliff/documents/xliff-core-1.1.xsd" version="1.1">
  <file source-language="en" datatype="plaintext" original="">
    <body>
      <!-- Service Pack Migrations -->
      <trans-unit id="distupgrade.upgrade">
        <source>Service Pack Migration</source>
      </trans-unit>
      <trans-unit id="SP Migration">
        <source>SP Migration</source>
      </trans-unit>
      <!-- Setup -->
      <trans-unit id="spmigration.jsp.setup.title">
        <source>Service Pack Migration</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.setup.installed-products">
        <source>Installed Products:</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.setup.target-products">
        <source>Target Products:</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.setup.base-channel">
        <source>Target Base Channel:</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.setup.mandatory-channels">
        <source>Mandatory Child Channels:</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.setup.optional-channels">
        <source>Optional Child Channels:</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.setup.none">
        <source>None</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.setup.submit">
        <source>Schedule Migration</source>
      </trans-unit>
      <!-- Errors -->
      <trans-unit id="spmigration.jsp.error.unsupported">
        <source>Service Pack Migrations are supported for &lt;strong&gt;SUSE&lt;/strong&gt; systems only.</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.error.update-zypp-plugin">
        <source>Service Pack Migrations are currently not supported for this system, please install the latest version of &lt;strong&gt;zypp-plugin-spacewalk&lt;/strong&gt;.</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.error.scheduled">
        <source>A Service Pack Migration is already &lt;a href="/network/systems/details/history/event.pxt?sid={0}&amp;hid={1}"&gt;scheduled&lt;/a&gt; for this system.</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.error.up-to-date">
        <source>There is currently no Service Pack migration available for this system. Either it is already on the &lt;strong&gt;latest&lt;/strong&gt; Service Pack level or the base channel of the target product was not yet mirrored. Use &lt;strong&gt;mgr-ncc-sync --list&lt;/strong&gt; to identify missing channels.</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.error.missing-channels">
        <source>The following channels are &lt;strong&gt;mandatory&lt;/strong&gt; for this migration, please synchronize using &lt;strong&gt;mgr-ncc-sync -c&lt;/strong&gt;:</source>
      </trans-unit>
      <!-- Confirm -->
      <trans-unit id="spmigration.jsp.confirm.title">
        <source>Service Pack Migration - Confirm</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.confirm.description">
        <source>Please confirm below to schedule the migration of this system to the following products:</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.confirm.channels-after-migration">
        <source>Channel subscriptions after the migration:</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.confirm.back">
        <source>Go Back</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.confirm.submit.dry-run">
        <source>Dry Run</source>
      </trans-unit>
      <trans-unit id="spmigration.jsp.confirm.submit">
        <source>Confirm</source>
      </trans-unit>
      <!-- Messages -->
      <trans-unit id="spmigration.message.scheduled">
        <source>This system is &lt;a href="/network/systems/details/history/event.pxt?sid={0}&amp;hid={1}"&gt;scheduled&lt;/a&gt; to be migrated to &lt;strong&gt;{2}&lt;/strong&gt;.</source>
      </trans-unit>
      <trans-unit id="spmigration.message.scheduled.dry-run">
        <source>This system is &lt;a href="/network/systems/details/history/event.pxt?sid={0}&amp;hid={1}"&gt;scheduled&lt;/a&gt; for a dry run of the migration to &lt;strong&gt;{2}&lt;/strong&gt;.</source>
      </trans-unit>

      <!-- SUSE only taskomatic jobs -->
      <trans-unit id="task.status.mgr-register">
        <source>Run mgr-register</source>
      </trans-unit>
      <trans-unit id="task.status.repo-sync">
        <source>Repository Sync</source>
      </trans-unit>
      <trans-unit id="task.status.ssh-push">
        <source>SSH Server Push</source>
      </trans-unit>
      <trans-unit id="task.status.cve-server-channels">
        <source>CVE Server Channels</source>
      </trans-unit>
      <trans-unit id="bunch.jsp.description.mgr-register-bunch">
        <source>Registers SUSE Manager clients</source>
      </trans-unit>
      <trans-unit id="bunch.jsp.description.repo-sync-bunch">
        <source>Synchronizes SUSE Manager repositories</source>
      </trans-unit>
      <trans-unit id="bunch.jsp.description.ssh-push-bunch">
        <source>Manages clients via SSH Server Push</source>
      </trans-unit>
      <trans-unit id="bunch.jsp.description.cve-server-channels-bunch">
        <source>Generates data required for performing CVE audit queries</source>
      </trans-unit>

      <!-- Server Contact Methods -->
      <trans-unit id="server.contact-method.label">
        <source>Contact Method</source>
      </trans-unit>
      <trans-unit id="server.contact-method.default">
        <source>Pull</source>
      </trans-unit>
      <trans-unit id="server.contact-method.ssh-push">
        <source>Push via SSH</source>
      </trans-unit>
      <trans-unit id="server.contact-method.ssh-push-tunnel">
        <source>Push via SSH tunnel</source>
      </trans-unit>
      <trans-unit id="sdc.details.edit.contact-method.unentitled">
        <source>Contact method not available for this system.</source>
      </trans-unit>
      
      <!-- SSH Push Messages -->
      <trans-unit id="kickstart.schedule.invalid.contact.method">
        <source>Provisioning is currently not supported for systems managed with push via SSH tunnel.</source>
      </trans-unit>

      <!-- CVE Audit Messages -->
      <trans-unit id="cveaudit.nav.title">
        <source>CVE Audit</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.overview">
        <source>CVE Audit</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.description">
        <source>This page will display a list of systems with their patch status regarding a given CVE (Common Vulnerabilities and Exposures) number.</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.cvenumber">
        <source>CVE Number:</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.unknowncveidentifier">
        <source>The specified CVE number was not found. This can happen for very old or yet-unknown numbers, please also check it for possible typing errors.</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.cvenumber.auditsystem">
        <source>Audit systems</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.examples">
        <source>Examples:</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.example.list">
        <source>CVE-2013-3343, CVE-2013-3117</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.filters">
        <source>System patch status to search:</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.patchstatus.affectedpatchinapplicable">
        <source>Affected, patch available in a channel which is not assigned</source>
      </trans-unit>     
      <trans-unit id="cveaudit.jsp.patchstatus.affectedpatchapplicable">
        <source>Affected, patch available in an assigned channel</source>
      </trans-unit>     
      <trans-unit id="cveaudit.jsp.patchstatus.notaffected">
        <source>Not affected</source>
      </trans-unit>     
      <trans-unit id="cveaudit.jsp.patchstatus.patched">
        <source>Patched</source>
      </trans-unit>    
      <trans-unit id="cveaudit.jsp.noresults">
        <source>No system found.</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.patchstatus">
        <source>Patch status</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.system">
        <source>System</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.nextaction">
        <source>Next Action</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.candidate">
        <source>Only candidate is:</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.candidates">
        <source>Candidates are:</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.nextaction.affectedpatchinapplicable">
        <source>Add a channel subscription to this system.</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.nextaction.affectedpatchapplicable">
        <source>Install a new patch in this system. </source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.nextaction.notaffected">
        <source>No action needed (system does not have affected software installed).</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.nextaction.patched">
        <source>No action needed.</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.updatenotice.pre">
        <source>Please note that underlying data needed for this audit is updated nightly. If you registered systems very recently or changed channel subscriptions in the last 24 hours it is recommended that you</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.updatenotice.link">
        <source>schedule an extra CVE data update</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.updatenotice.post">
        <source>in order to ensure consistent results.</source>
      </trans-unit>
      <trans-unit id="cveaudit.jsp.updatenotice.non-admin">
        <source>Please note that underlying data needed for this audit is updated nightly. If systems were registered very recently or channel subscriptions have been changed in the last 24 hours it is recommended that an extra CVE data update is scheduled in order to ensure consistent results.</source>
      </trans-unit>
      <trans-unit id="patchStatus">
        <source>Patch Status</source>
      </trans-unit>
      <trans-unit id="systemName">
        <source>System Name</source>
      </trans-unit>
      <trans-unit id="patchAdvisory">
        <source>Patch Advisory</source>
      </trans-unit>
      <trans-unit id="channelName">
        <source>Channel Name</source>
      </trans-unit>

      <!-- CSV separator -->
      <trans-unit id="preferences.jsp.csvtitle">
        <source>CSV Files</source>
      </trans-unit>
      <trans-unit id="preferences.jsp.csvdesc">
        <source>Configure a separator character to be used in downloadable CSV files:</source>
      </trans-unit>
      <trans-unit id="preferences.jsp.csvcomma">
        <source>Comma (",", default)</source>
      </trans-unit>
      <trans-unit id="preferences.jsp.csvsemicolon">
        <source>Semicolon (";", compatible with Microsoft&#174; Excel&#174;)</source>
      </trans-unit>
    </body>
  </file>
