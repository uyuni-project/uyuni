.spacewalk-toolbar-h1
  .spacewalk-toolbar
  h1
    i.fa.fa-group
    | #{" " + orgName + " "}
    a(href="/docs/reference/home/home-overview.html" target="_blank")
      i.fa.fa-question-circle.spacewalk-help-link

#recurring-states

script(type='text/javascript').
    window.csrfToken = "#{csrf_token}";
    window.orgId = "#{orgId}";
    window.orgName = "#{orgName}";
    window.minions = !{minions};
    window.timezone = "#{h.renderTimezone()}";
    window.localTime = "#{h.renderLocalTime()}";

script(type='text/javascript').
    spaImportReactPage('state/recurring-states')
        .then(function(module) { module.renderer('recurring-states') });
