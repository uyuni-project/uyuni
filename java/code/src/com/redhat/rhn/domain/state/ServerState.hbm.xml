<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "classpath://org/hibernate/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.redhat.rhn.domain.state.ServerState" table="suseServerState">
        <id name="id" type="long" column="id">
            <generator class="sequence">
                <param name="sequence">suse_server_state_id_seq</param>
            </generator>
        </id>
        <many-to-one name="server" column="server_id" not-null="true" class="com.redhat.rhn.domain.server.Server"/>
        <property name="packageStateGroupId" column="package_state_group_id" type="long" not-null="true"/>
        <property name="revision" column="revision" type="long" not-null="true"/>
        <property name="created" column="created" type="timestamp" insert="false" update="false"/>
    </class>
</hibernate-mapping>
