<?xml version="1.0" encoding="UTF-8"?>
<pxt-passthrough>
  <pxt-use class="Grail::Frame" />
  <pxt-use class="Sniglets::Users" />
  <pxt-use class="Sniglets::Channel" />

  <pxt-use class="Sniglets::HTML" />

  <grail-canvas-template base="/templates/c.pxt" mode="main_canvas">

<pxt-include file="/network/software/channels/manage/manage_channels_header.pxi" />

    <h2>
      <rhn-icon type="header-errata-set"/> Patchsets
    </h2>

    <pxt-formvar>

    <ul>
      <li>
        <a href="/rhn/channels/manage/solaris/ListPatchSets.do?cid={formvar:cid}">
	  List/Remove Patchsets
	</a>
      </li>

      <li>
        <a href="/network/software/channels/manage/patchsets/add.pxt?cid={formvar:cid}&amp;view_channel=no_channels&amp;pxt:trap=rhn:empty_set&amp;set_label=packages_to_add">
          Add Patchsets
        </a>
      </li>

    </ul>
	   
    </pxt-formvar>
  </grail-canvas-template>
</pxt-passthrough>
