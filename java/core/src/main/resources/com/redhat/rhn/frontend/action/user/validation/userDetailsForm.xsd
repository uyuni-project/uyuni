<?xml version="1.0"?>

<schema targetNamespace="http://rhn.redhat.com"
	xmlns="http://www.w3.org/1999/XMLSchema" xmlns:rhn="http://rhn.redhat.com">
    <attribute name="firstNames">
		<simpleType baseType="string">
			<minLength value="1" />
			<maxLength value="128" />
		</simpleType>
	</attribute>
    <attribute name="lastName">
        <simpleType baseType="string">
            <minLength value="1"/>
            <maxLength value="128"/>
        </simpleType>
    </attribute>
</schema>
