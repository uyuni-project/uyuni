BEGIN
    FOR idx IN (SELECT index_name FROM user_indexes WHERE index_name = 'SUSE_IMGSTORE_LABEL_UQ')
    LOOP
        EXECUTE IMMEDIATE 'DROP INDEX ' || idx.index_name;
    END LOOP;
END;
/

CREATE UNIQUE INDEX suse_imgstore_label_uq
    ON suseImageStore (org_id, label)
        TABLESPACE [[2m_tbs]];
